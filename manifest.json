{
    "manifest_version": 2,
    "name": "Xtrakt",
    "version": "1.0",
  
    "description": "Extrakt urls from any one page at a time",
  
    "icons": {
      "48": "extract-48.png"
    },
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": {
        "48": "extract-48.png"
      }
    },
    "background": {
      "scripts": ["extrakt_back.js"],
      "persistent": false
    },
    "permissions": [
      "tabs",
      "<all_urls>"
    ]
    
  }
  